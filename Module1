seq1 = "ACTGATCAC"
seq2 = "TTAGCTCAC"
def count_matches():
    if len(seq1) != len(seq2):
        raise ValueError("Sequences must be of the same length")
            score = 0
    for i in range(len(seq1)):
        if seq1[i] == seq2[i]:
            score += 1
    print(score)
def max_contiguous_chain():
    max_chain = 0
    current_chain = 0
for i in range(len(seq1)):
        if seq1[i] == seq2[i]:
            current_chain += 1
            max_chain = max(max_chain, current_chain)
        else:
            current_chain = 0
print(max_chain)
count_matches()
max_contiguous_chain()

